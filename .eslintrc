{
  "rules": {
    "indent": [
      2,
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ],
    "no-console": 0,
    "no-unused-vars": 1,
    "strict": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "globals": {
    "jQuery": true,
    "$": true,
    "Vue": true,
    "THREE": true,
    "_": true
  }
}
